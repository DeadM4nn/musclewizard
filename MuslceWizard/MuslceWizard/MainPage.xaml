<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MuslceWizard.MainPage"
             BackgroundImageSource="Front.png">

    <StackLayout>
        <Button Margin="24, 29, 0, 0"
        Text="Chest"
        HeightRequest="41"
        WidthRequest="105"
        HorizontalOptions="Start"
                >
        </Button>

        <Button Margin="10, 27, 0, 0"
        Text="Biceps"
        HeightRequest="41"
        WidthRequest="105"
        HorizontalOptions="Start"
                >
        </Button>

        <Button Margin="276, 43, 0, 0"
        Text="Abs"
        HeightRequest="41"
        WidthRequest="105"
        HorizontalOptions="Start"
                >
        </Button>

        <Button Margin="276, 47, 0, 90"
        Text="Quads"
        HeightRequest="41"
        WidthRequest="105"
        HorizontalOptions="Start"
                >
        </Button>

        <Button
        Text="Behind"
        Clicked="ToMuscleBack"
        HeightRequest="91"
                >
        </Button>
        
        <Button
        Text="User Info"
        HeightRequest="91"
                >
        </Button>        
        
        <Button
        Text="Calorie and Protein Intake"
        HeightRequest="91"
                >
        </Button>
    </StackLayout>

</ContentPage>
